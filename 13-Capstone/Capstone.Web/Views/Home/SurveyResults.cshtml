@model List<SurveyResultViewModel>

<link href="/css/surveyresultresults.css" rel="stylesheet">

@{
    ViewData["Title"] = "SurveyResults";
}

<h2>SurveyResults</h2>

<h4>This is a survey result page!</h4>

<div class="top-ranked-parks">
    @{
        foreach (SurveyResultViewModel item in Model)
        {
            <div class="parks">
                <div class="park-image">
                <a asp-action="Details" asp-route-data="item.ParkCode"><img src="~/images/parks/@(item.ParkCode).jpg" class="park-image"/></a>
            </div>
                <div class="park-info">
                    <div class="park-name">
                        <p>@item.ParkName</p>
                    </div>
                    <div class="total-votes">
                        <p>Total Votes: @item.VoteCount</p>
                    </div>
                </div>
            </div>
        }
    }
</div>
